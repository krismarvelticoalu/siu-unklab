generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model mataKuliah {
  idMk    String @id @map("id_mk") @db.VarChar(10)
  namaMk  String @map("nama_mk") @db.VarChar(50)
  ruangan String @db.VarChar(25)
  paralel String @db.Char(1)

  @@map("mata_kuliah")
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model mahasiswa {
  nim                  String    @id @db.VarChar(15)
  nama                 String?   @db.VarChar(255)
  nomor_registrasi     String?   @db.VarChar(10)
  jenis_kelamin        String?   @db.VarChar(10)
  tempat_tanggal_lahir DateTime? @db.Date
  status_akademik      String?   @db.VarChar(15)
  fakultas             String?   @db.VarChar(30)
  program_studi        String?   @db.VarChar(30)
  alamat               String?   @db.VarChar(35)
}
