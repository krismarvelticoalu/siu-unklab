generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model mataKuliah {
  idMk   String @id @db.VarChar(10) @map("id_mk")
  namaMk String @db.VarChar(50) @map("nama_mk")
  ruangan String @db.VarChar(25)
  paralel String @db.Char(1)

  @@map("mata_kuliah")
}
